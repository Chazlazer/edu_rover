<link name = "dummy_link">
    <collision>
        <origin xyz = "0 0 0" rpy = "0 0 0" />
        <geometry>
            <box size = "0.15 0.1 0.036"/>
        </geometry>
    </collision>
    <visual>
        <geometry>
            <box size = "0.01 0.01 0.01"/>
        </geometry>
    </visual>
    </link>

    <link name = "base_link">            
        <inertial>
            <origin xyz = "0 0 0" rpy = "0 0 0" />
            <mass value = "0.08"/>
            <inertia ixx = "0.00015864" ixy = "0.0" ixz = "0.0" iyy = "7.53066666667e-05" iyz = "0.0" izz = "0.0002166666667"/>
