<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="wheel_gazebo" params="number">
    <gazebo reference="wheel_${number}_link">
      <selfCollide>false</selfCollide>
      <mu1 value="1.2" />
      <mu2 value="1.2" />
      <material>Gazebo/Black</material>
    </gazebo>
  </xacro:macro>
</robot>

